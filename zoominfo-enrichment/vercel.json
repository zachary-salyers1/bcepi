{
    "version": 2,
    "crons": [
        {
            "path": "/api/cron/enrich-contacts",
            "schedule": "0 */2 * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/public/index.html"
        },
        {
            "source": "/dashboard",
            "destination": "/public/index.html"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ]
}